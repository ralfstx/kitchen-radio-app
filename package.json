{
  "name": "kitchen-radio-app",
  "version": "0.0.1",
  "main": "build/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ralfstx/kitchen-radio-app.git"
  },
  "license": "MIT",
  "scripts": {
    "build:ios": "rm -rf build/ && cp babel-ios.json .babelrc && babel --copy-files --out-dir build/ src/",
    "build:android": "rm -rf build/ && cp babel-android.json .babelrc && babel --copy-files --out-dir build/ src/",
    "test": "cp babel-test.json .babelrc && mocha --compilers js:babel-core/register 'test/**/*.spec.js'",
    "start": "hs -c-1"
  },
  "dependencies": {
    "core-js": "^2.2.1",
    "tabris": "https://tabrisjs.com/downloads/nightly/tabris.tgz",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.0",
    "babel-preset-es2015": "^6.5.0",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "sinon": "^1.17.3"
  }
}
